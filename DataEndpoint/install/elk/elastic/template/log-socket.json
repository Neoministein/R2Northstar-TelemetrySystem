{
  "index_patterns": [
    "log-socket-*"
  ],
  "priority": 500,
  "composed_of": ["searchable","log"],
  "template": {
    "settings": {
      "number_of_shards": "1",
      "auto_expand_replicas": "0-1"
    },
    "mappings": {
      "properties": {
        "incoming": {
          "type": "long"
        },
        "outgoing": {
          "type": "long"
        },
        "initiator": {
          "type": "keyword"
        },
        "context": {
          "type": "keyword"
        }
      }
    },
    "aliases": {
      "log-socket": {}
    }
  },
  "version": 3
}