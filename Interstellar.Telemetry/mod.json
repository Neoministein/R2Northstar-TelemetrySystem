{
	"Name" : "Interstellar.Telemetry",
	"Description" : "",
	"Version": "1.0.0",
	"LoadPriority": 10,
	"ConVars": [
		{
			"Name": "ts-hostname",
			"DefaultValue": "127.0.0.1:8090"
		},
		{
			"Name": "ts-apiKey",
			"DefaultValue": ""
		},
		{
			"Name": "ts-tags",
			"DefaultValue": ""
		},
		{
			"Name": "ts-tls",
			"DefaultValue": "false"
		},
		{
			"Name": "ts-milliSecBetweenState",
			"DefaultValue": "100"
		},
		{
			"Name": "ts-stopEventsAfterMilliSec",
			"DefaultValue": "1500"
		},
		{
			"Name": "ts-secToWaitForBackend",
			"DefaultValue": "5"
		},
		{
			"Name": "ts-recordNpc",
			"DefaultValue": "1"
		}
	],
	"Scripts": [
		{
			"Path": "_Telemetry.gnut",
			"RunOn": "SERVER && MP",
			"ServerCallback": {
				"Before": "ITS_init"
			}
		},
		{
			"Path": "_TelemetryUtil.gnut",
			"RunOn": "SERVER && MP"
		}
	]
}